{
  "name": "webservice-with-node",
  "version": "0.0.1",
  "description": "Backend service for Gitly",
  "main": "dist/app.js",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5",
    "firebase-functions-test": "^0.2.0"
  },
  "scripts": {
    "dev": "tsc -p . && nodemon src/app.ts",
    "build": "tsc",
